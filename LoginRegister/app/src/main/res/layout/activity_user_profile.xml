<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ca.synergyforce.ashiq.loginregister.UserProfileActivity">

    <TextView
        android:id="@+id/textViewWelcome"
        android:layout_width="161dp"
        android:layout_height="45dp"
        android:text="Welcome"
        android:textColor="@android:color/holo_green_light"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView_userName"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="18dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/buttonLogout"
        android:layout_marginLeft="16dp" />

    <Button
        android:id="@+id/buttonLogout"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:background="@android:color/holo_green_dark"
        android:text="Logout"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="37dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="37dp" />

    <TextView
        android:id="@+id/textView_userName"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:text="UserName"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/editText_age"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView_Age"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonLogout"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="36dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp" />

    <EditText
        android:id="@+id/editText_Name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView_userName"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewWelcome"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView_userName"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp" />

    <TextView
        android:id="@+id/textView_Age"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="Age"
        android:textSize="18sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintRight_toRightOf="@+id/textView_userName"
        android:layout_marginTop="177dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="10dp" />

    <EditText
        android:id="@+id/editText_age"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Age"
        android:inputType="number"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView_Email"
        app:layout_constraintRight_toRightOf="@+id/editText_email"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@+id/editText_Name"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="39dp"
        app:layout_constraintLeft_toLeftOf="@+id/editText_email" />

    <TextView
        android:id="@+id/textView_Email"
        android:layout_width="0dp"
        android:layout_height="34dp"
        android:text="Email"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/editText_email"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView_Address"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="21dp"
        android:layout_marginTop="114dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_userName"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="115dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="21dp" />

    <EditText
        android:id="@+id/editText_email"
        android:layout_width="0dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="6dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_Email"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView_Address"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="6dp" />

    <TextView
        android:id="@+id/textView_Phone"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Phone"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/editText_address"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="13dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText_phone"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="13dp" />

    <EditText
        android:id="@+id/editText_phone"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="phone"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/editText_address"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/editText_email"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="27dp"
        app:layout_constraintLeft_toRightOf="@+id/textView_Phone"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="16dp" />

    <TextView
        android:id="@+id/textView_Address"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:text="Address"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/editText_address"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/editText_address"
        android:layout_marginEnd="7dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editText_address"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="7dp" />

    <EditText
        android:id="@+id/editText_address"
        android:layout_width="246dp"
        android:layout_height="70dp"
        android:ems="10"
        android:hint="address"
        android:inputType="textPersonName"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="17dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="54dp"
        android:layout_marginRight="17dp" />
</android.support.constraint.ConstraintLayout>
